{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"opener:default",
		{
			"identifier": "http:default",
			"allow": [
				"https://trackmania.io/api/*",
				"https://core.trackmania.nadeo.live/maps/*/file",
				"https://trackmania.exchange/api/*"
			]
		}
	]
}
